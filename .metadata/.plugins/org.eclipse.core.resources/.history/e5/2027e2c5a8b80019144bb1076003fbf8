package quest3;

public class promoKit {

	String id;
	video vid;
	garment gar;
	book words;
	promoKit(String Id, video Vid, garment Gar)
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public video getVid() {
		return vid;
	}
	public void setVid(video vid) {
		this.vid = vid;
	}
	public garment getGar() {
		return gar;
	}
	public void setGar(garment cloth) {
		this.gar = cloth;
	}
	public book getWords() {
		return words;
	}
	public void setWords(book words) {
		this.words = words;
	}
	
}
