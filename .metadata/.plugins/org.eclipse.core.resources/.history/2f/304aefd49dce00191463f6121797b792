package ex8_1;

public class GarageDoor {
	
	public void up() {
		System.out.println("GD is up");
	}
	public void down() {
		System.out.println("GD is down");
		
	}
	public void stop() {
		System.out.println("GD is stop");
	}
	public void lightOn() {
		System.out.println("GD light is on");
	}
	public void lightOff() {
		System.out.println("GD light is off");
	}

}
