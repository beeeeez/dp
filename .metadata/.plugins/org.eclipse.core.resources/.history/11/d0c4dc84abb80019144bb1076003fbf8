package quest3;

public class manKitBuilder extends promoKitBuilder{

	promoKit manKit;
	
	public void buildVideoPart() {
		vid = new video();
		vid.genre="Action";		
	}
	public void buildGarmentPart() {
		garment gar = new garment();
		gar.type="Sombrero";
		return gar;
		
	}
	public void buildBookPart() {
		book boo = new book();
		boo.title="Man for Men";
		return boo;
	}
	public void getPromoKit() {
		video vid = buildVideoPart();
		garment gar = buildGarmentPart();
		book boo = buildBookPart();
		manKit= new promoKit("man's kit", vid, gar, boo);
		return manKit;
	}
	
}
