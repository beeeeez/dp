package ex7_1;

public class Small extends SizeDec{
	private Beverage bev;
	private String size;
	public Small(Beverage bev) {
		this.bev=bev;
		this.size="Sm";
		
	}
	public String getSize(){
		return size;
	}
	public void setSize(String size) {
		this.size=size;
	}
	public double cost() {
		return 0;
	}
}
