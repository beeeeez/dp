package ex7_1;

public abstract class Beverage {
	String desc = "Unknown Beverage";
	String size = "";
	public Beverage() {
	}

	public String getDesc() {
		return desc;
	}
	public abstract double cost();
	public String getSize() {
		return "";
	}
	public String setSize(String size) {
		this.size=size;
	}
	
}
