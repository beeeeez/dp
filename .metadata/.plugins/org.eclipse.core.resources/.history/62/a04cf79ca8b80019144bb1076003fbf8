package quest3;

public class promoKit {

	String id;
	video vid;
	garment cloth;
	book words;
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public video getVid() {
		return vid;
	}
	public void setVid(video vid) {
		this.vid = vid;
	}
	public garment getCloth() {
		return cloth;
	}
	public void setCloth(garment cloth) {
		this.cloth = cloth;
	}
	public book getWords() {
		return words;
	}
	public void setWords(book words) {
		this.words = words;
	}
	
}
