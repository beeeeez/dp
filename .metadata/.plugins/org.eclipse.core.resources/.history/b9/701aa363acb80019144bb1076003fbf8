package quest3;

public class womanKitBuilder extends promoKitBuilder {

promoKit womKit;
	
	public void buildVideoPart() {
		video vid = new video();
		vid.genre="Romance";		
	}
	public void buildGarmentPart() {
		garment gar = new garment();
		gar.type="Dress";
		
	}
	public void buildBookPart() {
		book boo = new book();
		boo.title="Girls for Gals";
	}
	public promoKit getPromoKit() {
		buildVideoPart();
		buildGarmentPart();
		buildBookPart();
		womKit= new promoKit("woman's kit", vid, gar, boo);
		return womKit;
	}
	
}
