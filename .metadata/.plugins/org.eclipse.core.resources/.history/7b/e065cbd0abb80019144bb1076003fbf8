package quest3;

public abstract class womanKitBuilder {

promoKit womKit;
	
	public video buildVideoPart() {
		video vid = new video();
		vid.genre="Romance";
		return vid;
		
	}
	public garment buildGarmentPart() {
		garment gar = new garment();
		gar.type="Dress";
		return gar;
		
	}
	public book buildBookPart() {
		book boo = new book();
		boo.title="Girls for Gals";
		return boo;
	}
	public promoKit getPromoKit() {
		video vid = buildVideoPart();
		garment gar = buildGarmentPart();
		book boo = buildBookPart();
		womKit= new promoKit("woman's kit", vid, gar, boo);
		return womKit;
	}
	
}
